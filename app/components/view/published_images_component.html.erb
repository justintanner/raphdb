<div class="container-images">
  <div class="row pt-3 gx-3" id="published-images">
    <%= render View::PublishedImagesCardComponent.with_collection(@items, view: @view, offset: @pagy.offset) %>
  </div>
  <div class="row">
    <div class="col text-center">
      <%= turbo_frame_tag "page_operations" %>
      <%= render View::LoadMoreComponent.new(tab: "images", query: @query, next_page: @pagy.next) %>
    </div>
  </div>
</div>

