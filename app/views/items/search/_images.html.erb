<%= turbo_frame_tag "page_handler" %>

<div class="container-images">
  <%= turbo_stream_from "images_items_turbo" %>
  <div class="row pt-3 gx-3" id="images-items">
    <%= render partial: "items/search/images_items", locals: { items: items, pagy: pagy } %>
  </div>
  <div class="row">
    <div class="col text-center">
      <%= render "items/search/images_load_more", pagy: pagy, tab: "images" %>
    </div>
  </div>
</div>