<%= turbo_frame_tag "filter_operations" do %>
  <%= turbo_stream.remove "filter_uuid_#{@uuid}" %>

  <%= turbo_stream.replace "empty_filters_message" do %>
    <%= turbo_frame_tag "empty_filters_message" do %>
      <%= render "editor/filters/empty_filters_message" %>
    <% end %>
  <% end %>
<% end %>
